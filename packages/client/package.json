{
  "name": "@kickjump/client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "dev": "npm run dev:client",
    "lint": "npm run lint:ts && npm run lint:js",
    "lint:ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "lint:js": "eslint \"**/*.js\" --ignore-path .eslintignore",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false",
    "posttest": "istanbul check-coverage --statement 75 --branch 50 --function 50",
    "postinstall": "npm run typings && npm run build",
    "typings": "rimraf typings/ && typings install"
  },
  "keywords": [],
  "author": "Ifiok Jr. <ifiokotung@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "awesome-typescript-loader": "2.1.0",
    "babel-core": "6.11.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "copy-webpack-plugin": "3.0.1",
    "cross-env": "2.0.0",
    "eslint": "3.1.1",
    "imports-loader": "0.6.5",
    "istanbul": "1.1.0-alpha.1",
    "istanbul-instrumenter-loader": "0.2.0",
    "json-loader": "0.5.4",
    "karma": "1.1.1",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.0",
    "karma-mocha": "1.1.1",
    "karma-remap-istanbul": "0.1.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-sourcemap-writer": "0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "rimraf": "2.5.3",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "source-map-support": "0.4.2",
    "ts-helpers": "1.1.1",
    "tslint": "3.13.0",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "typings": "1.3.1",
    "webpack": "2.1.0-beta.19",
    "webpack-dev-server": "2.1.0-beta.0"
  },
  "dependencies": {
    "@reactivex/rxjs": "5.0.0-beta.10",
    "rxjs": "5.0.0-beta.10"
  }
}
