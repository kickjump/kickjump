    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.10",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.3",
    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
