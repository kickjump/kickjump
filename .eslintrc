{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": ["babel", "security", "json", "mocha"],
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "optionalDependencies": false}],
    "semi" : [2, "always"],
    "max-len": [2, 120, 2],
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 1
  }
}
